{
  "ts-node": {
    "compilerOptions": {
      "module": "CommonJS"
    }
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "ESNext",
    "target": "ES5",
    "allowJs": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "strict": false
  },
  "exclude": ["node_modules"]
}
